{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": false,
      "baseUrl": ".",
      "paths": {
          "components/*": [
              "src/components/*"
          ],
          "Constants": [
              "src/constants.js"
          ],
          "screens/*": [
              "src/screens/*"
          ],
          "state/*": [
              "src/state/*"
          ],
          "services/*": [
              "src/services/*"
          ],
          "assets/*": [
              "src/assets/*"
          ],
          "navigation/*": [
              "src/navigation/*"
          ],
          "appearance/*": [
            "src/appearance/*"
          ],
          "Config": [
              "config.js"
          ]
      }
  },
  "exclude": [
      "node_modules",
      "dist"
  ]
}